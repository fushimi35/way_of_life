<%= render partial:'shared/error_message', locals: { object: board } %>
<%= form_with(model: board, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :title %>
    <%= form.text_field :title, class:'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :body %>
    <%= form.text_area :body, class:'form-control', rows:'10' %>
  </div>
  <div class="form-group">
    <%= form.label :board_image %>
    <%= form.file_field :board_image, class:'form-control' %>
    <%= form.hidden_field :board_image_cache, class:'form-control' %>
  </div>
  <%= form.submit class:'btn btn-primary' %>
<% end %>
